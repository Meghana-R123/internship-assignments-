exp 2 

cd path/to/your/folder
git init
notepad file1.txt
git add file1.txt
git commit -m "Initial commit"
git branch
git branch feature-branch
git checkout feature-branch
notepad file1.txt
git add file1.txt
git commit -m "Added new feature in feature branch"
git checkout master
notepad file1.txt
git add file1.txt
git commit -m "Updated file in master branch"
git merge feature-branch
notepad file1.txt
git add file1.txt
git commit -m "Resolved merge conflict"
git status
git log
git branch -d feature-branch

